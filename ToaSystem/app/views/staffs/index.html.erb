<p id="notice"><%= notice %></p>

<h1>Staffs</h1>

<table>
  <thead>
    <tr>
      <th>Per cedula</th>
      <th>Per apellidos</th>
      <th>Per nombres</th>
      <th>Per fecha nacimiento</th>
      <th>Per telefono</th>
      <th>Fecha creacion</th>
      <th>Fecha actualizacion</th>
      <th>Id usuario creacion</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @staffs.each do |staff| %>
      <tr>
        <td><%= staff.PER_CEDULA %></td>
        <td><%= staff.PER_APELLIDOS %></td>
        <td><%= staff.PER_NOMBRES %></td>
        <td><%= staff.PER_FECHA_NACIMIENTO %></td>
        <td><%= staff.PER_TELEFONO %></td>
        <td><%= staff.FECHA_CREACION %></td>
        <td><%= staff.FECHA_ACTUALIZACION %></td>
        <td><%= staff.ID_USUARIO_CREACION %></td>
        <td><%= link_to 'Show', staff %></td>
        <td><%= link_to 'Edit', edit_staff_path(staff) %></td>
        <td><%= link_to 'Destroy', staff, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Staff', new_staff_path %>
