<p id="notice"><%= notice %></p>
<div class="container">
  <h1>PACIENTES</h1>
  <table class="table table-striped">
    <thead>
      <tr>
        <th>Nro. Historia</th>
        <th>Cédula</th>
        <th>Apellido Paterno</th>
        <th>Apellido Materno</th>
        <th>Nombre</th>
        <th>Fecha de Nacimiento</th>
        <th>Nacionalidad</th>
        <th>Género</th>
        <th>Estado Civil</th>
        <th>Instrucción</th>
        <th>Pac ocupacion</th>
        <th>Teléfono</th>
        <th>Grupo Sanguíneo</th>
        <!--<th>Fecha creacion</th>
        <th>Fecha actualizacion</th>
        <th>Id usuario creacion</th>!-->
        <th colspan="3"></th>
      </tr>
    </thead>

    <tbody>
      <% @patients.each do |patient| %>
        <tr>
          <td><%= patient.PAC_HC %></td>
          <td><%= patient.PAC_CEDULA %></td>
          <td><%= patient.PAC_APELLIDO_PATERNO %></td>
          <td><%= patient.PAC_APELLIDO_MATERNO %></td>
          <td><%= patient.PAC_NOMBRES %></td>
          <td><%= patient.PAC_FECHA_NACIMIENTO %></td>
          <td><%= patient.PAC_NACIONALIDAD %></td>
          <td><%= patient.PAC_GENERO %></td>
          <td><%= patient.PAC_ESTADO_CIVIL %></td>
          <td><%= patient.PAC_INSTRUCCION %></td>
          <td><%= patient.PAC_OCUPACION %></td>
          <td><%= patient.PAC_TELEFONO %></td>
          <td><%= patient.PAC_GRUPO_SANGUINEO %></td>
          <!--<td><%= patient.FECHA_CREACION %></td>
          <td><%= patient.FECHA_ACTUALIZACION %></td>
          <td><%= patient.ID_USUARIO_CREACION %></td>!-->
          <td><%= link_to 'Mostrar', patient %></td>
          <td><%= link_to 'Editar', edit_patient_path(patient) %></td>
          <td><%= link_to 'Eliminar', patient, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
  <%= link_to 'NUEVO PACIENTE', new_patient_path %>
</div>
<br>
